<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    tools:context=".MainActivity">

    <Button
        style="@style/DefaultButton"
        android:text="Intent Send String"
        android:id="@+id/buttonIntentString"/>
    <Button
        android:layout_below="@id/buttonIntentString"
        style="@style/DefaultButton"
        android:text="Intent Send Int"
        android:id="@+id/buttonIntentInt"/>
    <Button
        android:layout_below="@id/buttonIntentInt"
        style="@style/DefaultButton"
        android:text="Intent Send Object"
        android:id="@+id/buttonIntentObject"/>
</RelativeLayout>